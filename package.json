{
  "name": "react-tailwind-grid-viewer",
  "description": "A React component for viewing a grid defined with tailwind css",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/react-tailwind-grid-viewer.umd.cjs",
  "module": "dist/react-tailwind-grid-viewer.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-tailwind-grid-viewer.js",
      "require": "./dist/react-tailwind-grid-viewer.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "clsx": "^2.1.1",
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "path": "^0.12.7",
    "postcss": "^8.4.41",
    "rollup-preserve-directives": "^1.1.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3"
  }
}
